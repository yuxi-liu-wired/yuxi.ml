@article{shiomiPossibleLinkBrain2022,
  title = {Possible Link between Brain Size and Flight Mode in Birds: {{Does}} Soaring Ease the Energetic Limitation of the Brain?},
  shorttitle = {Possible Link between Brain Size and Flight Mode in Birds},
  author = {Shiomi, Kozue},
  year = {2022},
  journal = {Evolution},
  volume = {76},
