@article{collinVerificationCrooksFluctuation2005,
  title = {Verification of the {{Crooks}} Fluctuation Theorem and Recovery of {{RNA}}                     Folding Free Energies},
  author = {Collin, D. and Ritort, F. and Jarzynski, C. and Smith, S. B. and Tinoco, I. and Bustamante, C.},
